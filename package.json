{
  "name": "auto-doc-system",
  "version": "1.0.0",
  "description": "Production-grade automated API documentation and versioning system with AST parsing and AI enhancement",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "cli": "node cli/index.js",
    "scan": "node cli/index.js scan",
    "build-docs": "node cli/index.js build-docs",
    "validate": "node cli/index.js validate",
    "version-bump": "node cli/index.js version-bump",
    "export-openapi": "node cli/index.js export-openapi",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "validate:shell": "bash scripts/validate-docs.sh"
  },
  "keywords": [
    "api",
    "documentation",
    "ast",
    "reflection",
    "versioning",
    "automation"
  ],
  "author": "MERN Stack Architect",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.23.6",
    "@babel/traverse": "^7.23.6",
    "@babel/types": "^7.23.6",
    "@google/generative-ai": "^0.2.1",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "ora": "^8.0.1",
    "semver": "^7.5.4",
    "simple-git": "^3.22.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
